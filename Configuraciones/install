echo '=============================// Copy .Xresources'
cp Xresources ~/.Xresources

echo '=============================// merge and load Xresources'
xrdb -merge .Xresources
xrdb -load .Xresources

echo '=============================// Cppy zshrc'
cp zshrc ~/.zshrc

echo '=============================// Copy terminator'
cp terminator/ ~/.config/terminator/
